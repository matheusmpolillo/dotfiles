{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "tsconfig.json",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint/eslint-plugin"],
    "rules": {
        "indent": ["off"],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["off"]
    }
}
